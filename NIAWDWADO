    if table.find(WHITELISTED, tostring(game.Players.LocalPlayer.PlayerGui.MainGui.StatusFrame["ClanName"].Text):split(" ")[2]) == nil then
       game.ReplicatedStorage.Reroll:FireServer("BP")
    else
       break
    end
