        loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
    end)
    
    MainSection:NewButton("Auto Reroll (Legendary and higher)", "auto reroll must have rerolls", function()
        local WHITELISTED = {"HaÃ±ma", "Songoku", "Kagutsuchi", "Sung", "Raijin", "Sanji", "Ryu", "Fujin", "Wu", "Kage", "Ohma", "Karu" ,"Akuma", "Mikazuchi", "Kure", "Kageyama"}
    
